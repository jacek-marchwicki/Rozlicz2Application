<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.label {
			font-weight: bold;
			font-size: 20px;
		}
		.edit {
			font-size: 8px;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Label ui:field="textLabel" styleName="style.important"></g:Label>
		<g:Anchor ui:field="editAnchor" styleName="style.edit" text="[edit]"></g:Anchor>
		<g:TextBox ui:field="textBox"></g:TextBox>
		<g:Label ui:field="errorLabel"></g:Label>
		<g:Button ui:field="saveButton" text="Save"></g:Button>
		<g:Button ui:field="cancelButton" text="Cancel"></g:Button>
	</g:HTMLPanel>
</ui:UiBinder> 