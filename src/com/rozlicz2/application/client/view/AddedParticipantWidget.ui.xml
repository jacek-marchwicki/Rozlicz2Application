<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="com.rozlicz2.application.client.resources.ApplicationResources" field="res"></ui:with>
	<ui:with type="com.rozlicz2.application.client.resources.ApplicationConstants" field="i18n"></ui:with>

	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.label {
			float: left;
		}
		.removeButton {
			float: left;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Label styleName="{style.label}" ui:field="userLabel"></g:Label>
		<g:Button styleName="{style.removeButton}" ui:field="removeButton" text="{i18n.removeUser}">
		
		</g:Button>
		<g:Image resource="{res.removeUser}"></g:Image>
		
	</g:HTMLPanel>
</ui:UiBinder> 