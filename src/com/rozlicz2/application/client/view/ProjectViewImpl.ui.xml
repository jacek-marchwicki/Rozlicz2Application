<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:com.rozlicz2.application.client.view"
	xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	
	<ui:with type="com.rozlicz2.application.client.resources.ApplicationConstants" field="i18n"></ui:with>
	
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<v:EditableLabelWidget ui:field="projectNameWidget"></v:EditableLabelWidget>
		<p1:CellList ui:field="expensesList"></p1:CellList>
		<g:Button ui:field="createExpenseButton" text="{i18n.newExpense}"></g:Button>
	</g:HTMLPanel>
</ui:UiBinder> 